{
  "name": "auth-service",
  "version": "0.0.1",
  "description": "Authentication and authorization microservice for Russian gaming platform",
  "author": "Gaming Platform Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:refactored": "node run-refactored-tests.js",
    "test:refactored:unit": "jest --config jest.config.js src/token/token.service.spec.ts src/auth/auth.service.*.spec.ts src/session/session.service.spec.ts",
    "test:refactored:e2e": "jest --config ./test/jest-e2e.json test/auth-flows-refactored.e2e-spec.ts test/redis-integration-refactored.spec.ts",
    "demo:tests": "node demo-refactored-tests.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d typeorm.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d typeorm.config.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d typeorm.config.ts",
    "db:init": "ts-node scripts/database.ts init",
    "db:status": "ts-node scripts/database.ts status",
    "db:reset": "ts-node scripts/database.ts reset",
    "db:test": "ts-node scripts/database.ts test",
    "services:check": "npx ts-node scripts/check-services.ts",
    "consistency:demo": "cross-env CONSISTENCY_CHECK_MODE=check npx ts-node scripts/consistency-check-demo.ts",
    "consistency:demo:repair": "cross-env CONSISTENCY_CHECK_MODE=repair npx ts-node scripts/consistency-check-demo.ts",
    "consistency:demo:report": "cross-env CONSISTENCY_CHECK_MODE=report npx ts-node scripts/consistency-check-demo.ts",
    "consistency:check": "cross-env CONSISTENCY_CHECK_MODE=check npx ts-node scripts/consistency-check.ts",
    "consistency:repair": "cross-env CONSISTENCY_CHECK_MODE=repair npx ts-node scripts/consistency-check.ts",
    "consistency:report": "cross-env CONSISTENCY_CHECK_MODE=report npx ts-node scripts/consistency-check.ts",
    "consistency:report:json": "cross-env CONSISTENCY_CHECK_MODE=report CONSISTENCY_CHECK_OUTPUT=json npx ts-node scripts/consistency-check.ts",
    "consistency:report:csv": "cross-env CONSISTENCY_CHECK_MODE=report CONSISTENCY_CHECK_OUTPUT=csv npx ts-node scripts/consistency-check.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/lru-cache": "^7.10.9",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lru-cache": "^11.2.2",
    "opossum": "^8.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
