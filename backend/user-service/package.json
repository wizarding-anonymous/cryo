{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User management microservice for Russian gaming platform",
  "author": "Cryo Platform Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:load": "cd load-tests && ./run-tests.sh",
    "test:load:quick": "k6 run --duration 2m --vus 100 load-tests/k6-load-test.js",
    "test:stress": "k6 run load-tests/k6-stress-test.js",
    "test:functional": "node test-service.js",
    "backup:full": "curl -X POST http://localhost:3001/backup/full -H 'Authorization: Bearer admin-token'",
    "backup:incremental": "curl -X POST http://localhost:3001/backup/incremental -H 'Authorization: Bearer admin-token'",
    "typeorm": "npx ts-node ./node_modules/typeorm/cli.js -d ./data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/infrastructure/persistence/migrations/Initial",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "docker:build": "docker build -f Dockerfile.prod -t user-service:latest .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t user-service:dev .",
    "docker:run": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:run:dev": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "k8s:deploy": "./scripts/deploy.sh",
    "k8s:deploy:staging": "./scripts/deploy.sh -e staging",
    "k8s:deploy:prod": "./scripts/deploy.sh -e production",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "clean": "rm -rf dist coverage node_modules/.cache",
    "precommit": "npm run lint:check && npm run format:check && npm run test:cov",
    "prepare": "echo 'Skipping husky install'"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "kafkajs": "^2.2.4",
    "opossum": "^9.0.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "passport-vkontakte": "^0.5.0",
    "passport-yandex": "^0.0.5",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "russian-requisites-validator": "^1.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.20.0"
  }
}
