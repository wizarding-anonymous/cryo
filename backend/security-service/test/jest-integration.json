{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testRegex": ".*\\.integration-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/setup-integration.ts"],
  "testEnvironment": "node",
  "testTimeout": 60000,
  "maxWorkers": 1,
  "forceExit": true,
  "detectOpenHandles": true
}