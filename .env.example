# PostgreSQL Database
POSTGRES_DB=user_service_db
POSTGRES_USER=user
POSTGRES_PASSWORD=password

# Redis Cache
REDIS_HOST=redis
REDIS_PORT=6379

# Kafka Broker
KAFKA_BROKER=kafka:29092

# Application Port
PORT=3000

# Frontend URL
FRONTEND_URL=http://localhost:3000

# JWT Settings
JWT_SECRET=your-super-secret-key-that-is-long
JWT_EXPIRES_IN=15m
# API Keys for Service Integration
DEVELOPER_PORTAL_API_KEY=dev-portal-service-key-change-in-production
INTERNAL_SERVICE_API_KEY=internal-service-key-change-in-production

# Integration Monitoring
INTEGRATION_MONITORING_ENABLED=true
HEALTH_CHECK_INTERVAL_MS=60000